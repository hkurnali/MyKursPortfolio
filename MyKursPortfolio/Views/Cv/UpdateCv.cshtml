@model MyKursPortfolio.Models.Cv
@{
    ViewBag.Title = "UpdateCv";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Cv Güncelleme Sayfası</h2>

<form method="post">

    <label>Cv ID</label>
   @Html.TextBoxFor(x=>x.CvID, new { @class = "form-control" })
    <label>Cv Url</label>
    @Html.TextBoxFor(x => x.CvURL, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Kaydet</button>
</form>
