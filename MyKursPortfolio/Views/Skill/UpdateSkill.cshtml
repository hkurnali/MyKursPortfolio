@model MyKursPortfolio.Models.Skill
@{
    ViewBag.Title = "UpdateSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yetenek Güncelleme Sayfası</h2>

<form method="post">

    <label>Yetenek ID</label>
   @Html.TextBoxFor(x=>x.SkillID,new{ @class = "form-control" })
    <label>Yetenek Adı</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br/>
    
    <button class="btn btn-success">Kaydet</button>
</form>
